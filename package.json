{
  "name": "jupyter-fs",
  "version": "0.0.1",
  "description": "A JupyterLab extension.",
  "author": "The jupyter-fs authors",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "tsc",
    "build:integrity": "jlpm install && jlpm build",
    "build:lab": "rimraf lab-dist && mkdirp lab-dist && cd lab-dist && npm pack ..",
    "build:all": "jlpm run build:integrity && jlpm run build:lab",
    "clean": "rimraf lib",
    "fix": "./node_modules/.bin/tslint --fix src/* src/*/*",
    "lint": "./node_modules/.bin/tslint './src/*.ts'",
    "prepublishOnly": "jlpm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{ts}",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/**/*.{json,}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "base": {
          "name": "jupyter-fs"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "extension": true,
    "schemaDir": "schema"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/docmanager": "^2.0.0",
    "@jupyterlab/docregistry": "^2.0.0",
    "@jupyterlab/filebrowser": "^2.0.0",
    "@jupyterlab/services": "^5.0.0",
    "@jupyterlab/settingregistry": "^2.0.0",
    "@jupyterlab/ui-components": "^2.0.0",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/disposable": "^1.3.5",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.1.6",
    "file-saver": "^2.0.2",
    "jszip": "^3.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24",
    "babel-jest": "^24.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24",
    "jest-transform-css": "^2.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24",
    "tslint": "^5.20.0",
    "typescript": "^3.7.0"
  }
}
